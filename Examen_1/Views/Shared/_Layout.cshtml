<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Examen_1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>

        <nav class="navbar navbar-light bg-light">
            <form class="container-fluid justify-content-start">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Dr Pool S.A.</a>
                <button class="btn btn-outline-info me-2" asp-area="" asp-controller="Paquete" asp-action="Index">
                    Paquetes
                </button>
            </form>
        </nav>

    </header>

    <br />
    <div class="container">
        <main role="main" class="pb-3">
            <div class="btn-group d-md-flex justify-content-md-center" role="group">
                <button type="button" class="btn btn-info" asp-area="" asp-controller="Paquete" asp-action="Index">Paso 1</button>
                <button type="button" class="btn btn-info" asp-area="" asp-controller="Paquete" asp-action="Index">Paso 2</button>
                <button type="button" class="btn btn-info" asp-area="" asp-controller="Paquete" asp-action="Index">Paso 3</button>
            </div>
            <br />

            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; Examen 1 <a asp-area="" asp-controller="Home" asp-action="Privacy">Joselyn Monge Granados</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
